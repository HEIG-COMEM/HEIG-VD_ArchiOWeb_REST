{
    "collectCoverage": true,
    "collectCoverageFrom": [
        "controllers/**/*.js",
        "middlewares/**/*.js",
        "models/**/*.js",
        "routes/**/*.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["clover", "json", "html", "lcov", "text"],
    "setupFilesAfterEnv": ["jest-extended/all"]
}
